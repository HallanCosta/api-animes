{
  "name": "api-anime",
  "version": "1.0.0",
  "description": "API crawler - animesorion.vip",
  "main": "index.js",
  "scripts": {
    "jest-clear": "jest --clearCache",
    "dev": "ts-node-dev --ignore-watch node_modules src/server.ts",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "keywords": [],
  "author": "HallanCosta",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/supertest": "^2.0.10",
    "jest": "^26.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "express": "^4.17.1"
  }
}
